  <scroller>
    <enabled>true</enabled>
    <connect>false</connect>
    <height>20</height>
    <graph_data_source>tweets</graph_data_source>
    <graph_selected_color>#3b90cf</graph_selected_color>
    <graph_selected_fill_color>#3b90cf</graph_selected_fill_color>
    <resize_button_style>dragger</resize_button_style>
    <resize_button_color>#d5d5d5</resize_button_color>
  </scroller>
  <balloon>
    <border_color>#1f7fd7</border_color>
