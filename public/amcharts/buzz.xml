      <main_drop_down selected="false"/>
      <compare_list_box selected="false"/>
      <csv>
        <reverse>false</reverse>
        <separator>,</separator>
        <date_format>YYYY-MM-DD</date_format>
        <decimal_separator>.</decimal_separator>
        <columns>
