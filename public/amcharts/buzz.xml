        <border_color>#3b90cf</border_color>
        <border_alpha/>
        <text_color/>
        <size/>
        <hide_period/>
      </events>
      <graphs>
        <graph>
          <axis/>
          <type>line</type>
          <connect>false</connect>
          <data_sources>
            <close>tweets</close>
          </data_sources>
          <period_value>average</period_value>
          <compare_source>tweets</compare_source>
          <title/>
          <color>#0077CC</color>
          <width>2</width>
          <alpha/>
          <fill_alpha>50</fill_alpha>
          <dashed/>
          <bullet/>
          <bullet_size/>
          <smoothed/>
